<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="border_color" format="color" />
    <declare-styleable name="ButtonColorPicker">
        <attr name="color" format="color" />
        <attr name="border_color" />
        <attr name="border_width" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MessageDialog">
        <attr name="title" />
        <attr name="message" format="string" />
        <attr name="positive_button_text" format="string" />
        <attr name="negative_button_text" format="string" />
    </declare-styleable>
    <declare-styleable name="NumberStepper">
        <attr name="value" format="integer" />
        <attr name="min_value" format="integer" />
        <attr name="max_value" format="integer" />
        <attr name="step" format="integer" />
        <attr name="backgroundTint" format="color" />
        <attr name="buttonTint" format="color" />
        <attr name="textColor" format="color" />
        <attr name="textAppearance" format="reference" />
    </declare-styleable>
</resources>
